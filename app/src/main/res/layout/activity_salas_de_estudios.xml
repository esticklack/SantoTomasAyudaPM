<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SalasDeEstudiosActivity">

    <include
        android:id="@+id/include2"
        layout="@layout/interfaz_principal"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-61dp" />

    <ImageView
        android:id="@+id/imgReference1"
        android:layout_width="253dp"
        android:layout_height="107dp"
        android:layout_marginTop="292dp"
        android:contentDescription="Imagen de referencia 1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/include2"
        app:srcCompat="@drawable/foto_prueba"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/imgReference2"
        android:layout_width="253dp"
        android:layout_height="107dp"
        android:layout_marginTop="420dp"
        android:contentDescription="Imagen de referencia 1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/include2"
        app:srcCompat="@drawable/foto_prueba"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/imgReference3"
        android:layout_width="253dp"
        android:layout_height="107dp"
        android:layout_marginTop="556dp"
        android:contentDescription="Imagen de referencia 1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/include2"
        app:srcCompat="@drawable/foto_prueba"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/imgReference4"
        android:layout_width="253dp"
        android:layout_height="107dp"
        android:layout_marginTop="684dp"
        android:contentDescription="Imagen de referencia 1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/include2"
        app:srcCompat="@drawable/foto_prueba"
        tools:ignore="HardcodedText,VisualLintBounds" />

    <TextView
        android:id="@+id/txtPrueba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SALAS DE ESTUDIO"
        android:textColor="@color/white"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/imgReference1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/include2"
        app:layout_constraintVertical_bias="0.847"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>