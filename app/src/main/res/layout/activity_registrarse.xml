<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistrarseActivity"
    tools:layout_editor_absoluteY="25dp">


    <include
        android:id="@+id/include"
        layout="@layout/interfaz_base"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-64dp" />

    <View
        android:id="@+id/view"
        android:layout_width="367dp"
        android:layout_height="419dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="406dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="89dp"
        android:background="@drawable/rdborder"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnRegistrarse"
        android:layout_width="257dp"
        android:layout_height="74dp"
        android:layout_marginBottom="112dp"
        android:backgroundTint="#1E5C4F"
        android:text="Registrarse"
        android:textColor="@color/white"
        android:textSize="17sp"
        android:translationZ="300dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/etNombre"
        android:layout_width="163dp"
        android:layout_height="61dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="144dp"
        android:autofillHints="emailAddress"
        android:background="@drawable/rdborder_edittext"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/nombre"
        android:inputType="text"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/etPasswordRegistro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.935" />

    <EditText
        android:id="@+id/etApellido"
        android:layout_width="163dp"
        android:layout_height="61dp"
        android:layout_marginBottom="144dp"
        android:autofillHints="emailAddress"
        android:background="@drawable/rdborder_edittext"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/apellido"
        android:inputType="text"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/etPasswordRegistro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.358"
        app:layout_constraintStart_toEndOf="@+id/etNombre"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.935" />

    <EditText
        android:id="@+id/etCorreoRegistro"
        android:layout_width="343dp"
        android:layout_height="62dp"
        android:layout_marginBottom="136dp"
        android:autofillHints="emailAddress"
        android:background="@drawable/rdborder_edittext"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/correo"
        android:inputType="textEmailAddress"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/btnRegistrarse"
        app:layout_constraintEnd_toEndOf="@+id/etApellido"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/etNombre"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/etPasswordRegistro"
        android:layout_width="343dp"
        android:layout_height="62dp"
        android:layout_marginBottom="32dp"
        android:autofillHints="emailAddress"
        android:background="@drawable/rdborder_edittext"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/password"
        android:inputType="textPassword"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/btnRegistrarse"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="393dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="836dp" />


</androidx.constraintlayout.widget.ConstraintLayout>